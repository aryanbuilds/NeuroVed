{
  "name": "dashdash",
  "version": "1.0.0",
  "description": "AI Doctor Dashboard with 3D Brain Segmentation",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install open --no-save",
    "start": "node start-servers.js",
    "start:frontend": "npm install open --no-save && node frontend/start-frontend.js",
    "start:3d-model": "cd backend/3d_segmentation_model && npm run dev",
    "start:gradio": "cd Ai_Doctor && python run_gradio_server.py",
    "open-browser": "npm install open --no-save && node open-browser.js"
  },
  "keywords": [
    "ai",
    "doctor",
    "dashboard",
    "3d",
    "segmentation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "open": "^8.4.2"
  }
} 